{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanlla and NodeJS",
  "main": "index.js",
  "author": "me",
  "browserslist": [
    "cover 99.5%"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.644.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mongoose": "^5.9.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "prebuild": "rm -rf build",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
